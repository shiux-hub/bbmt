{
  "name": "bbmt",
  "displayName": "BBMT",
  "type": "module",
  "version": "0.0.1",
  "description": "Browser bookmark management tool",
  "author": "Dropout",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "eslint": "eslint . --cache",
    "eslint:fix": "eslint . --cache --max-warnings 0 --fix",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.2",
    "plasmo": "0.88.0",
    "radash": "^12.1.0",
    "vue": "3.4.33",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/prompt-cli": "^19.3.1",
    "@commitlint/types": "^19.0.3",
    "@types/chrome": "0.0.268",
    "@types/node": "20.14.11",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "eslint": "npm:eslint-ts-patch@9.5.0-0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-ts-patch": "9.5.0-0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "typescript": "5.5.3"
  },
  "lint-staged": {
    "*.{scss,css,vue,ts,md}": [
      "eslint --fix"
    ]
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "bookmarks"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "pnpm@8.15.6+sha1.8105075ad0aa306138be128c017e9c41e28ecffa"
}
